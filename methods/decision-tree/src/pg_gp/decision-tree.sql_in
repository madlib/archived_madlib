AS 'MODULE_PATHNAME', 'array_add'
LANGUAGE C IMMUTABLE;

CREATE OR REPLACE FUNCTION MADLIB_SCHEMA.min ( 
  x double precision,
  y double precision
) RETURNS double precision
AS $$
BEGIN
	IF (X <= Y) THEN
  		RETURN x;
  	ELSE 
  		RETURN y;
  	END IF;
END;
$$ LANGUAGE 'plpgsql';

