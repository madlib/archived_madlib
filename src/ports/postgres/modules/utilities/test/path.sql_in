DROP TABLE IF EXISTS weblog, path_output, path_output_rows CASCADE;

CREATE TABLE weblog (event_timestamp TIMESTAMP,
            user_id INT,
            age_group INT,
            income_group INT,
            gender TEXT,
            region TEXT,
            household_size INT,
            click_event INT,
            purchase_event INT,
            revenue FLOAT,
            margin FLOAT);

