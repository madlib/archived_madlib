(20,1,3.49,5),
(21,1,2.42,4),
(22,1,4.01,3),
(23,1,4.91,2),
(24,1,5,1);


SELECT assert(
    relative_error(coef, ARRAY[2.5444, 1.6718]) < 1e-4 AND
    relative_error(loglikelihood, -37.8532) < 1e-2,
    'Cox-Proportional hazards (unique time of death): Wrong results'
) FROM (
		SELECT (madlib.cox_prop_hazards(
    'leukemia', 'ARRAY[grp, wbc]', 'timeDeath', 20, 'newton',  0.001
		)).*
) q;

