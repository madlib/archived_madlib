CREATE TABLE wsr_test AS
SELECT (wsr_test(
    x - y,
    2 * 2^(-52) * greatest(x,y)
    ORDER BY abs(x - y)
)).*
FROM test_wsr;

SELECT * FROM wsr_test;
SELECT assert(
    statistic = 105.5 AND
    rank_sum_pos = 105.5 AND
    rank_sum_neg = 194.5 AND
    num = 24 AND
    relative_error(z_statistic, -1.272) < 0.001,
    'Wilcoxon signed-rank: Wrong results'
) FROM wsr_test;
